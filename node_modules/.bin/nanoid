#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/eGovFrame-4.0.0/workspace.edu/Ltest/on-air-platform/final/node_modules/nanoid/bin/node_modules:/mnt/c/eGovFrame-4.0.0/workspace.edu/Ltest/on-air-platform/final/node_modules/nanoid/node_modules:/mnt/c/eGovFrame-4.0.0/workspace.edu/Ltest/on-air-platform/final/node_modules:/mnt/c/eGovFrame-4.0.0/workspace.edu/Ltest/on-air-platform/node_modules:/mnt/c/eGovFrame-4.0.0/workspace.edu/Ltest/node_modules:/mnt/c/eGovFrame-4.0.0/workspace.edu/node_modules:/mnt/c/eGovFrame-4.0.0/node_modules:/mnt/c/node_modules:/mnt/c/eGovFrame-4.0.0/workspace.edu/Ltest/on-air-platform/final/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/eGovFrame-4.0.0/workspace.edu/Ltest/on-air-platform/final/node_modules/nanoid/bin/node_modules:/mnt/c/eGovFrame-4.0.0/workspace.edu/Ltest/on-air-platform/final/node_modules/nanoid/node_modules:/mnt/c/eGovFrame-4.0.0/workspace.edu/Ltest/on-air-platform/final/node_modules:/mnt/c/eGovFrame-4.0.0/workspace.edu/Ltest/on-air-platform/node_modules:/mnt/c/eGovFrame-4.0.0/workspace.edu/Ltest/node_modules:/mnt/c/eGovFrame-4.0.0/workspace.edu/node_modules:/mnt/c/eGovFrame-4.0.0/node_modules:/mnt/c/node_modules:/mnt/c/eGovFrame-4.0.0/workspace.edu/Ltest/on-air-platform/final/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nanoid/bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../nanoid/bin/nanoid.cjs" "$@"
fi
